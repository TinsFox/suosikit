{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": ["node_modules"],
  "include": [".plasmo/index.d.ts", "./**/*.ts", "./**/*.tsx"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~*": ["./*"],
      "config/*": ["./config"],
      "@/*": ["./*"],
      "routers/*": ["src/routers/*"],
      "@/lib/*": ["./lib/*"],
      "components/*": ["src/components/*"]
    }
  }
}
